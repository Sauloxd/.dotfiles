{
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.formatOnSaveMode": "modifications",
  "javascript.format.enable": false,
  "typescript.format.enable": false,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "typescript.tsdk": "node_modules/typescript/lib",
  "git.ignoreLimitWarning": true,
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.detectIndentation": false,
  "editor.lightbulb.enabled": false,
  "editor.tabSize": 2,
  "explorer.autoReveal": false,
  "extensions.autoUpdate": false,
  "files.trimTrailingWhitespace": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "terminal.integrated.rendererType": "dom",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.tsserver.log": "verbose",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#05bcfa",
    "titleBar.activeForeground": "#15202b",
    "activityBar.background": "#37c9fb",
    "activityBar.activeBorder": "#db04a4",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#db04a4",
    "activityBarBadge.foreground": "#e7e7e7",
    "titleBar.inactiveBackground": "#05bcfa99",
    "titleBar.inactiveForeground": "#15202b99",
    "statusBarItem.hoverBackground": "#0496c8",
    "statusBar.background": "#8BFCBB",
    "statusBar.noFolderBackground": "#8BFCBB",
    "statusBar.debuggingBackground": "#8BFCBB",
    "statusBar.foreground": "#434C5E"
  },
  "workbench.editor.enablePreview": false,
  "editor.minimap.enabled": false,
  "workbench.editor.showTabs": true,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "files.associations": {
    "*.mjml": "slim"
  },
  "workbench.activityBar.visible": false,
  "vim.easymotion": true,
  "vim.useSystemClipboard": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "vspacecode.space"
      ]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [
        "<space>"
      ],
      "commands": [
        "vspacecode.space"
      ]
    }
  ],

}
